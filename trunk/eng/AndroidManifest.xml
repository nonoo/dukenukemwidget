<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.nonoo.dukenukem.widget"
      android:versionName="1.2" android:versionCode="3">
		<application android:icon="@drawable/icon" android:label="@string/app_name" android:description="@string/app_description" android:debuggable="false">
			<receiver android:name="DukeNukemWidgetProvider" >
			    <intent-filter>
			        <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
			        <action android:name="android.appwidget.action.APPWIDGET_DISABLED" />
			    </intent-filter>
			    <meta-data android:name="android.appwidget.provider"
			               android:resource="@xml/appwidgetprovider" />
			</receiver>
		
			<service android:name="DukeNukemWidgetService"></service>
</application>

<uses-sdk android:minSdkVersion="3"></uses-sdk>
</manifest> 